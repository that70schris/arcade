{
  "colors": {
    "activityBarBadge.background": "#7be",
    "editor.background": "#222",
    "editor.foreground": "#eee",
    "editor.lineHighlightBackground": "#333",
    "editor.selectionBackground": "#555",
    "editorCursor.foreground": "#fff",
    "editorIndentGuide.background": "#444",
    "editorMarkerNavigationWarning.background": "#eee",
    "editorOverviewRuler.warningForeground": "#eee",
    "editorWarning.foreground": "#ccc",
    "list.warningForeground": "#ef7",
    "sideBarTitle.foreground": "#bbbbbb",
    "statusBar.background": "#666"
  },
  "name": "Arcade",
  "tokenColors": [
    {
      "scope": [
        "entity.name.function",
        "meta.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#9ef"
      }
    },
    {
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#a9f"
      }
    },
    {
      "scope": [
        "constant",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#37c"
      }
    },
    {
      "scope": [
        "entity.name",
        "entity.other.inherited-class",
        "meta.type",
        "support.class",
        "support.type",
        "source.css entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#ec7"
      }
    },
    {
      "scope": [
        "string",
        "meta.attribute-selector"
      ],
      "settings": {
        "foreground": "#7e7"
      }
    },
    {
      "scope": [
        "entity.name.tag.reference",
        "meta.attribute.unrecognized entity.other.attribute-name",
        "meta.tag",
        "support.variable",
        "variable"
      ],
      "settings": {
        "foreground": "#7be"
      }
    },
    {
      "scope": [
        "comment",
        "comment punctuation",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#777"
      }
    },
    {
      "scope": [
        "constant.character",
        "keyword.control",
        "keyword.other.unit",
        "punctuation.definition.string",
        "punctuation.definition.type",
        "punctuation.support",
        "punctuation.terminator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#999"
      }
    },
    {
      "scope": [
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.switch",
        "keyword.control.trycatch",
        "keyword.operator.new",
        "meta.attribute.unrecognized.*ngIf entity.other.attribute-name",
        "meta.attribute.unrecognized.*ngFor entity.other.attribute-name",
        "meta.attribute.unrecognized.*ngSwitchCase entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#f9f"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.object-literal.key",
        "support.type.property-name",
        "source.yaml entity.name.tag"
      ],
      "settings": {
        "foreground": "#b97"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.other",
        "meta.brace",
        "punctuation.separator"
      ],
      "settings": {
        "foreground": "#eee"
      }
    }
  ],
  "type": "dark"
}
