{
  "colors": {
    "activityBarBadge.background": "#37c",
    "editor.background": "#222",
    "editor.foreground": "#eee",
    "editor.lineHighlightBackground": "#111",
    "editor.selectionBackground": "#555",
    "editorCursor.foreground": "#fff",
    "editorGutter.background": "#333",
    "editorIndentGuide.background": "#444",
    "editorMarkerNavigationWarning.background": "#eee",
    "editorOverviewRuler.warningForeground": "#eee",
    "editorWarning.foreground": "#ccc",
    "list.warningForeground": "#ef7",
    "sideBarTitle.foreground": "#bbb",
    "statusBar.background": "#666"
  },
  "name": "Arcade",
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.string",
        "punctuation.definition.tag",
        "punctuation.definition.template-expression",
        "punctuation.definition.type",
        "punctuation.definition.typeparameters",
        "punctuation.support",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#777"
      }
    },
    {
      "scope": [
        "constant.character",
        "constant.other.placeholder",
        "keyword.control",
        "keyword.other.unit",
        "storage.modifier",
        "support.type.object.module"
      ],
      "settings": {
        "foreground": "#aaa"
      }
    },
    {
      "scope": [
        "keyword.operator.expression.delete",
        "keyword.operator.new",
        "storage"
      ],
      "settings": {
        "foreground": "#a9f"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "keyword.other.special-method",
        "meta.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#9ef"
      }
    },
    {
      "scope": [
        "constant",
        "support.constant",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#37c"
      }
    },
    {
      "scope": [
        "entity.name",
        "entity.other.inherited-class",
        "meta.type",
        "support.class",
        "support.constant.math",
        "support.type",
        "source.css entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#ec7"
      }
    },

    {
      "scope": [
        "entity.name.tag.reference",
        "entity.name.type.anchor",
        "meta.attribute.unrecognized entity.other.attribute-name",
        "support.variable",
        "variable"
      ],
      "settings": {
        "foreground": "#7be"
      }
    },
    {
      "scope": [
        "constant.language.merge.yaml",
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.switch",
        "keyword.control.trycatch",
        "keyword.other.debugger",
        "meta.attribute.unrecognized.*ngIf entity.other.attribute-name",
        "meta.attribute.unrecognized.*ngFor entity.other.attribute-name",
        "meta.attribute.unrecognized.*ngSwitchCase entity.other.attribute-name",
        "punctuation.definition.anchor"
      ],
      "settings": {
        "foreground": "#f9f"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.object-literal.key",
        "support.type.property-name",
        "source.yaml entity.name.tag"
      ],
      "settings": {
        "foreground": "#b97"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#7e7"
      }
    }
  ],
  "type": "dark"
}
