{
  "injectionSelector": "L:meta.attribute.unrecognized",
  "patterns": [
    {
      "match": "\\[\\w+\\]=\\[\"'].*[\"']",
      "name": "meta.attribute.input"
    },
    {
      "match": "\\(\\w+\\)=\\[\"'].*[\"']",
      "name": "meta.attribute.output"
    },
    {
      "match": "\\*\\w+=\\[\"'].*[\"']",
      "name": "meta.attribute.control"
    }
  ],
  "scopeName": "html.injection"
}
