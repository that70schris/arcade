{
  "author": {
    "email": "chris@bailey.codes",
    "name": "Chris Bailey",
    "url": "https://bailey.codes"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "text.html"
        ],
        "path": "./dist/html.json",
        "scopeName": "injection.html"
      },
      {
        "injectTo": [
          "source.css"
        ],
        "path": "./dist/css.json",
        "scopeName": "injection.css"
      },
      {
        "injectTo": [
          "source.nginx"
        ],
        "path": "./dist/nginx.json",
        "scopeName": "injection.nginx"
      }
    ],
    "themes": [
      {
        "label": "Arcade",
        "path": "./dist/Arcade.json",
        "uiTheme": "vs-dark"
      }
    ]
  },
  "description": "Somewhere between neon and pastel",
  "devDependencies": {
    "@types/css": "0.0.31",
    "@types/node": "13.1.6",
    "@types/plist": "3.0.2",
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "css": "2.2.4",
    "eslint": "6.8.0",
    "husky": "4.0.7",
    "lint-staged": "9.5.0",
    "plist": "3.0.1",
    "prettier": "1.19.1",
    "sass": "1.24.4",
    "stylelint": "13.0.0",
    "stylelint-order": "4.0.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.13.0",
    "ts-node": "8.6.2",
    "typescript": "3.7.4",
    "vsce": "1.71.0"
  },
  "displayName": "Arcade",
  "engines": {
    "vscode": "^1.36.0"
  },
  "name": "arcade",
  "publisher": "that70schris",
  "repository": {
    "type": "git",
    "url": "https://github.com/that70schris/arcade"
  },
  "version": "0.11.5"
}
