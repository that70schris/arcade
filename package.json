{
  "author": {
    "email": "chris@bailey.codes",
    "name": "Chris Bailey",
    "url": "https://bailey.codes"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "text.html"
        ],
        "path": "./dist/html.json",
        "scopeName": "injection.html"
      },
      {
        "injectTo": [
          "source.css"
        ],
        "path": "./dist/css.json",
        "scopeName": "injection.css"
      },
      {
        "injectTo": [
          "source.nginx"
        ],
        "path": "./dist/nginx.json",
        "scopeName": "injection.nginx"
      }
    ],
    "themes": [
      {
        "label": "Arcade",
        "path": "./dist/Arcade.json",
        "uiTheme": "vs-dark"
      }
    ]
  },
  "description": "Somewhere between neon and pastel",
  "devDependencies": {
    "@types/css": "0.0.31",
    "@types/node": "12.12.6",
    "@types/plist": "3.0.2",
    "@typescript-eslint/parser": "2.6.1",
    "css": "2.2.4",
    "eslint": "6.6.0",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "plist": "3.0.1",
    "prettier": "1.19.1",
    "sass": "1.23.3",
    "stylelint": "11.1.1",
    "stylelint-order": "3.1.1",
    "stylelint-prettier": "1.1.1",
    "stylelint-scss": "3.12.1",
    "ts-node": "8.4.1",
    "tslint": "5.20.1",
    "typescript": "3.7.2",
    "vsce": "1.69.0"
  },
  "displayName": "Arcade",
  "engines": {
    "vscode": "^1.36.0"
  },
  "name": "arcade",
  "publisher": "that70schris",
  "repository": {
    "type": "git",
    "url": "https://github.com/that70schris/arcade"
  },
  "version": "0.9.21"
}
